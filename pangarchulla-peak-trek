    transition: 0.25s ease-in-out;
    text-decoration: none;
}

.contact-fixed img,
.whatsapp-fixed img {
    width: 22px;
}

/* Hide class */
.hide {
    opacity: 0;
    pointer-events: none;
}

/* Mobile Responsive */
@media screen and (max-width: 768px) {
    .contact-fixed,
    .whatsapp-fixed {
        padding: 8px 14px;
        font-size: 14px;
        gap: 6px;
        border-radius: 25px 0 0 25px;
    }

    .contact-fixed img,
    .whatsapp-fixed img {
        width: 18px;
    }
}



      </style>



