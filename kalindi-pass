                              <div class="detailfaq-answer">
                              
                              <div class="detailtabs">
                              <div class="detailtab active" data-tab="day1">Day 1</div>
                              <div class="detailtab" data-tab="day2">Day 2</div>
                              <div class="detailtab" data-tab="day3">Day 3</div>
                              <div class="detailtab" data-tab="day4">Day 4</div>
                              <div class="detailtab" data-tab="day5">Day 5</div>
