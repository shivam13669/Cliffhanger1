<!-- Enquiry Form -->
<form class="exp-form" method="POST" action="">
    <input type="text" class="exp-input" name="full_name" placeholder="Full Name *" required>
    <input type="email" class="exp-input" name="email" placeholder="Email *" required>
    
    <div class="exp-form-row">
        <input type="tel" class="exp-input" name="phone" placeholder="Your Phone with country code *" required>
    </div>

    <div class="exp-form-row">
        <label for="travel_date">Trek Date</label>
        <input type="date" class="exp-input" name="travel_date" id="travel_date" required>
    </div>

    <input type="text" class="exp-input" name="trek_name" placeholder="Trek Name *" required>

    <textarea rows="3" class="exp-textarea" name="message" placeholder="Message..." id="message"></textarea>

    <!-- Google reCAPTCHA -->
    <div class="g-recaptcha" data-sitekey="6LcKhNcrAAAAAF2J78HjJBR2t8JJXZEpL0kfxdu1"></div>

    <button type="submit" name="enquiry_submit" class="exp-btn">Send Enquiry</button>
</form>

<!-- reCAPTCHA script -->
<script src="https://www.google.com/recaptcha/api.js" async defer></script>

<!-- Client-side sanitize message input -->
<script>
document.getElementById('message').addEventListener('input', function () {
    let input = this.value;

    // Remove HTML tags
    input = input.replace(/<[^>]*>/g, '');

    // Remove links/domains
    input = input.replace(/(?:https?:\/\/|www\.|\.com|\.in|\.net|\.org|\.co|\.me|\.info)/gi, '');

    // Allow only English letters, numbers, spaces and common punctuation
    input = input.replace(/[^a-zA-Z0-9\s.,?!@\-]/g, '');

    this.value = input;
});
</script>
